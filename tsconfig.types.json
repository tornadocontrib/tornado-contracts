{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["es5", "es6", "es2021", "esnext"],
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./typechain-types",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["./typechain-types/**/*"],
  "exclude": ["node_modules"]
}